package koreait.day08;

public class Score {
	private int korean;
	private int english;
	private int science;
	
	void setKorean(int korean) {
		this.korean=korean;
		//메소드 인자로 전달받은 값을 클래스 전역변수 korean에 저장.(대입)
		//this 는 setKorean메소드를 실행시키는 객체
	}
	int getkorean() {
		return korean;
	}
	void setEnglish(int english) {
		this.english = english;
	}
	int getEnglish() {
		return english;
	
	
	}
	void setGrade(String grade) {
		this.grade = grade;
		
	}
	String getGrade() {
		return grade;
	}

}
